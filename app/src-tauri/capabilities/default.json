{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default permissions for the application",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "http://*:*" },      
        { "url": "http://*:*/*" },    
        { "url": "http://*/*" }       
      ]
    }
  ]
}
